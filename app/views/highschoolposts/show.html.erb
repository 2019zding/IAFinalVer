<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @highschoolpost.title %>
</p>
<% (0...@highschoolpost.images.count).each do |image| %>
<div style="width:300px, height:300px;">
	<%= image_tag(@highschoolpost.images[image], style:'width:20%; height:auto') %>
</div>
<% end %>
<%= link_to 'Download', @highschoolpost.file %>
<p>
  <strong>Body:</strong>
  <%= @highschoolpost.body %>
</p>

<%= link_to 'Edit', edit_highschoolpost_path(@highschoolpost) %> |
<%= link_to 'Back', highschoolposts_path %>
